{
  "name": "cs-nestrand",
  "version": "1.0.2",
  "description": "A random number generator for NestJS",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && tsc",
    "prepublish": "npm run build"
  },
  "author": "Paras Mendiratta <code.snooker@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "7.0.13",
    "@nestjs/core": "7.0.13",
    "@nestjs/schematics": "7.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5"
  },
  "devDependencies": {
    "@nestjs/testing": "7.0.13",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.5",
    "jest": "26.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.4",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
